  <types>
    <dataTypes/>
    <pous>
      <pou name="ElevatorControl" pouType="program">
        <interface>
          <localVars>
            <variable name="System_On" address="%IX1.0">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Floor1_Call" address="%IX1.1">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Floor2_Call" address="%IX1.2">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Floor1_Limit_Switch" address="%IX1.3">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Floor2_Limit_Switch" address="%IX1.4">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Floor1_Open_Door_Motor" address="%QX1.0">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Floor2_Open_Door_Motor" address="%QX1.1">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Floor1_Close_Door_Motor" address="%QX1.2">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Floor2_Close_Door_Motor" address="%QX1.3">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Floor1_Open_Door_Limit_Switch" address="%IX1.5">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Floor2_Open_Door_Limit_Switch" address="%IX1.6">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Floor1_Close_Door_Limit_Switch" address="%IX1.7">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Floor2_Close_Door_Limit_Switch" address="%IX1.8">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Pulley_Motor_Up" address="%QX1.4">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Pulley_Motor_Down" address="%QX1.5">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Door_Timer" address="%TM1">
              <type>
                <TON/>
              </type>
            </variable>
            <variable name="Sequencer" address="%IW1.0">
              <type>
                <INT/>
              </type>
            </variable>
          </localVars>
        </interface>
        <body>
          <ST>
            <name>ElevatorControl</name>
            <If>
              <condition>
                <variable>System_On</variable>
              </condition>
              <ST>
                <Assign>
                  <variable>Sequencer</variable>
                  <INT>1</INT>
                </Assign>
              </ST>
            </If>
            <If>
              <condition>
                <variable>Sequencer</variable>
                <operand2>
                  <INT>1</INT>
                </operand2>
              </condition>
              <ST>
                <If>
                  <condition>
                    <AND>
                      <operand1>
                        <variable>Floor2_Call</variable>
                      </operand1>
                      <operand2>
                        <NOT>
                          <variable>Floor2_Limit_Switch</variable>
                        </NOT>
                      </operand2>
                    </AND>
                  </condition>
                  <ST>
                    <Assign>
                      <variable>Pulley_Motor_Up</variable>
                      <BOOL>true</BOOL>
                    </Assign>
                    <Assign>
                      <variable>Sequencer</variable>
                      <INT>2</INT>
                    </Assign>
                  </ST>
                  <ElseIf>
                    <condition>
                      <AND>
                        <operand1>
                          <variable>Floor1_Call</variable>
                        </operand1>
                        <operand2>
                          <NOT>
                            <variable>Floor1_Limit_Switch</variable>
                          </NOT>
                        </operand2>
                      </AND>
                    </condition>
                    <ST>
                      <Assign>
                        <variable>Pulley_Motor_Down</variable>
                        <BOOL>true</BOOL>
                      </Assign>
                      <Assign>
                        <variable>Sequencer</variable>
                        <INT>3</INT>
                      </Assign>
                    </ST>
                  </ElseIf>
                </If>
              </ST>
            </If>
            <If>
              <condition>
                <variable>Sequencer</variable>
                <operand2>
                  <INT>2</INT>
                </operand2>
              </condition>
              <ST>
                <If>
                  <condition>
                    <variable>Floor2_Limit_Switch</variable>
                  </condition>
                  <ST>
                    <Assign>
                      <variable>Pulley_Motor_Up</variable>
                      <BOOL>false</BOOL>
                    </Assign>
                    <Assign>
                      <variable>Floor2_Open_Door_Motor</variable>
                      <BOOL>true</BOOL>
                    </Assign>
                    <If>
                      <condition>
                        <variable>Floor2_Open_Door_Limit_Switch</variable>
                      </condition>
                      <ST>
                        <Assign>
                          <variable>Floor2_Open_Door_Motor</variable>
                          <BOOL>false</BOOL>
                        </Assign>
                        <Call>
                          <function>TON</function>
                          <variable>Door_Timer</variable>
                          <CallArgument>
                            <type>
                              <BOOL/>
                            </type>
                            <BOOL>true</BOOL>
                          </CallArgument>
                        </Call>
                        <Assign>
                          <variable>Sequencer</variable>
                          <INT>4</INT>
                        </Assign>
                      </ST>
                    </If>
                  </ST>
                </If>
              </ST>
            </If>
            <If>
              <condition>
                <variable>Sequencer</variable>
                <operand2>
                  <INT>3</INT>
                </operand2>
              </condition>
              <ST>
                <If>
                  <condition>
                    <variable>Floor1_Limit_Switch</variable>
                  </condition>
                  <ST>
                    <Assign>
                      <variable>Pulley_Motor_Down</variable>
                      <BOOL>false</BOOL>
                    </Assign>
                    <Assign>
                      <variable>Floor1_Open_Door_Motor</variable>
                      <BOOL>true</BOOL>
                    </Assign>
                    <If>
                      <condition>
                        <variable>Floor1_Open_Door_Limit_Switch</variable>
                      </condition>
                      <ST>
                        <Assign>
                          <variable>Floor1_Open_Door_Motor</variable>
                          <BOOL>false</BOOL>
                        </Assign>
                        <Call>
                          <function>TON</function>
                          <variable>Door_Timer</variable>
                          <CallArgument>
                            <type>
                              <BOOL/>
                            </type>
                            <BOOL>true</BOOL>
                          </CallArgument>
                        </Call>
                        <Assign>
                          <variable>Sequencer</variable>
                          <INT>5</INT>
                        </Assign>
                      </ST>
                    </If>
                  </ST>
                </If>
              </ST>
            </If>
            <If>
              <condition>
                <variable>Sequencer</variable>
                <operand2>
                  <INT>4</INT>
                </operand2>
              </condition>
              <ST>
                <If>
                  <condition>
                    <variable>Door_Timer.Q</variable>
                  </condition>
                  <ST>
                    <Assign>
                      <variable>Floor2_Close_Door_Motor</variable>
                      <BOOL>true</BOOL>
                    </Assign>
                    <If>
                      <condition>
                        <variable>Floor2_Close_Door_Limit_Switch</variable>
                      </condition>
                      <ST>
                        <Assign>
                          <variable>Floor2_Close_Door_Motor</variable>
                          <BOOL>false</BOOL>
                        </Assign>
                        <Call>
                          <function>TON</function>
                          <variable>Door_Timer</variable>
                          <CallArgument>
                            <type>
                              <BOOL/>
                            </type>
                            <BOOL>false</BOOL>
                          </CallArgument>
                        </Call>
                        <Assign>
                          <variable>Sequencer</variable>
                          <INT>1</INT>
                        </Assign>
                      </ST>
                    </If>
                  </ST>
                </If>
              </ST>
            </If>
            <If>
              <condition>
                <variable>Sequencer</variable>
                <operand2>
                  <INT>5</INT>
                </operand2>
              </condition>
              <ST>
                <If>
                  <condition>
                    <variable>Door_Timer.Q</variable>
                  </condition>
                  <ST>
                    <Assign>
                      <variable>Floor1_Close_Door_Motor</variable>
                      <BOOL>true</BOOL>
                    </Assign>
                    <If>
                      <condition>
                        <variable>Floor1_Close_Door_Limit_Switch</variable>
                      </condition>
                      <ST>
                        <Assign>
                          <variable>Floor1_Close_Door_Motor</variable>
                          <BOOL>false</BOOL>
                        </Assign>
                        <Call>
                          <function>TON</function>
                          <variable>Door_Timer</variable>
                          <CallArgument>
                            <type>
                              <BOOL/>
                            </type>
                            <BOOL>false</BOOL>
                          </CallArgument>
                        </Call>
                        <Assign>
                          <variable>Sequencer</variable>
                          <INT>1</INT>
                        </Assign>
                      </ST>
                    </If>
                  </ST>
                </If>
              </ST>
            </If>
          </ST>
        </body>
      </pou>
    </pous>
