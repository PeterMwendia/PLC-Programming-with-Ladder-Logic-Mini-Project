  <types>
    <dataTypes/>
    <pous>
      <pou name="AutomaticDoorControl" pouType="program">
        <interface>
          <localVars>
            <variable name="Main_Switch" address="%IX1.0">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Object_Detection_Sensor" address="%IX1.1">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Open_Limit_Switch" address="%IX1.2">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Close_Limit_Switch" address="%IX1.3">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Open_Door_Motor" address="%QX1.0">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Close_Door_Motor" address="%QX1.1">
              <type>
                <BOOL/>
              </type>
            </variable>
            <variable name="Door_Timer" address="%TM1">
              <type>
                <TON/>
              </type>
            </variable>
            <variable name="Sequencer" address="%IW1.0">
              <type>
                <INT/>
              </type>
            </variable>
            <variable name="Open_Switch_Count" address="%IW2.0">
              <type>
                <INT/>
              </type>
              <initialValue>
                <INT>0</INT>
              </initialValue>
            </variable>
          </localVars>
        </interface>
        <body>
          <ST>
            <name>AutomaticDoorControl</name>
            <If>
              <condition>
                <variable>Main_Switch</variable>
              </condition>
              <ST>
                <If>
                  <condition>
                    <variable>Sequencer</variable>
                    <operand2>
                      <INT>0</INT>
                    </operand2>
                  </condition>
                  <ST>
                    <If>
                      <condition>
                        <variable>Close_Limit_Switch</variable>
                      </condition>
                      <ST>
                        <Assign>
                          <variable>Open_Door_Motor</variable>
                          <BOOL>false</BOOL>
                        </Assign>
                        <Assign>
                          <variable>Close_Door_Motor</variable>
                          <BOOL>false</BOOL>
                        </Assign>
                        <Assign>
                          <variable>Sequencer</variable>
                          <INT>1</INT>
                        </Assign>
                      </ST>
                    </If>
                  </ST>
                </If>
                <If>
                  <condition>
                    <variable>Sequencer</variable>
                    <operand2>
                      <INT>1</INT>
                    </operand2>
                  </condition>
                  <ST>
                    <If>
                      <condition>
                        <AND>
                          <operand1>
                            <variable>Object_Detection_Sensor</variable>
                          </operand1>
                          <operand2>
                            <NOT>
                              <variable>Open_Limit_Switch</variable>
                            </NOT>
                          </operand2>
                        </AND>
                      </condition>
                      <ST>
                        <Assign>
                          <variable>Open_Door_Motor</variable>
                          <BOOL>true</BOOL>
                        </Assign>
                        <Assign>
                          <variable>Close_Door_Motor</variable>
                          <BOOL>false</BOOL>
                        </Assign>
                      </ST>
                    </If>
                    <ElseIf>
                      <condition>
                        <variable>Open_Limit_Switch</variable>
                      </condition>
                      <ST>
                        <Assign>
                          <variable>Open_Door_Motor</variable>
                          <BOOL>false</BOOL>
                        </Assign>
                        <Call>
                          <function>TON</function>
                          <variable>Door_Timer</variable>
                          <CallArgument>
                            <type>
                              <BOOL/>
                            </type>
                            <BOOL>true</BOOL>
                          </CallArgument>
                        </Call>
                        <Assign>
                          <variable>Open_Switch_Count</variable>
                          <variable>Open_Switch_Count</variable>
                          <INT>+</INT>
                          <INT>1</INT>
                        </Assign>
                        <If>
                          <condition>
                            <variable>Open_Switch_Count</variable>
                            <operand2>
                              <INT>5</INT>
                            </operand2>
                            <operator>&lt;</operator>
                          </condition>
                          <ST>
                            <Assign>
                              <variable>Sequencer</variable>
                              <INT>2</INT>
                            </Assign>
                          </ST>
                          <Else>
                            <ST>
                              <Assign>
                                <variable>Sequencer</variable>
                                <INT>4</INT>
                              </Assign>
                            </ST>
                          </Else>
                        </If>
                      </ST>
                    </ElseIf>
                  </ST>
                </If>
                <If>
                  <condition>
                    <variable>Sequencer</variable>
                    <operand2>
                      <INT>2</INT>
                    </operand2>
                  </condition>
                  <ST>
                    <If>
                      <condition>
                        <variable>Door_Timer.Q</variable>
                      </condition>
                      <ST>
                        <Assign>
                          <variable>Close_Door_Motor</variable>
                          <BOOL>true</BOOL>
                        </Assign>
                        <Call>
                          <function>TON</function>
                          <variable>Door_Timer</variable>
                          <CallArgument>
                            <type>
                              <BOOL/>
                            </type>
                            <BOOL>false</BOOL>
                          </CallArgument>
                        </Call>
                        <Assign>
                          <variable>Sequencer</variable>
                          <INT>3</INT>
                        </Assign>
                      </ST>
                    </If>
                  </ST>
                </If>
                <If>
                  <condition>
                    <variable>Sequencer</variable>
                    <operand2>
                      <INT>3</INT>
                    </operand2>
                  </condition>
                  <ST>
                    <If>
                      <condition>
                        <variable>Close_Limit_Switch</variable>
                      </condition>
                      <ST>
                        <Assign>
                          <variable>Close_Door_Motor</variable>
                          <BOOL>false</BOOL>
                        </Assign>
                        <Assign>
                          <variable>Sequencer</variable>
                          <INT>1</INT>
                        </Assign>
                      </ST>
                    </If>
                  </ST>
                </If>
                <If>
                  <condition>
                    <variable>Sequencer</variable>
                    <operand2>
                      <INT>4</INT>
                    </operand2>
                  </condition>
                  <ST>
                    <If>
                      <condition>
                        <variable>Open_Limit_Switch</variable>
                      </condition>
                      <ST>
                        <Assign>
                          <variable>Open_Door_Motor</variable>
                          <BOOL>false</BOOL>
                        </Assign>
                        <Assign>
                          <variable>Close_Door_Motor</variable>
                          <BOOL>true</BOOL>
                        </Assign>
                      </ST>
                      <ElseIf>
                        <condition>
                          <variable>Close_Limit_Switch</variable>
                        </condition>
                        <ST>
                          <Assign>
                            <variable>Close_Door_Motor</variable>
                            <BOOL>false</BOOL>
                          </Assign>
                          <Assign>
                            <variable>Open_Door_Motor</variable>
                            <BOOL>true</BOOL>
                          </Assign>
                        </ST>
                      </ElseIf>
                    </If>
                  </ST>
                </If>
              </ST>
            </If>
          </ST>
        </body>
      </pou>
    </pous>
